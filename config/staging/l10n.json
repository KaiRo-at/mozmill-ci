{
    "pulse": {
        "applabel": "mozmill_l10n",
        "platforms": [
            "linux64",
            "macosx",
            "macosx64",
            "win32"
        ],
        "products": [
            "firefox"
        ],
        "tags": [
            "dep",
            "l10n"
        ],
        "trees": [
            "mozilla-aurora"
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "APPLICATION": {
                    "key": "product"
                },
                "BRANCH": {
                    "key": "tree"
                },
                "BUILD_ID": {
                    "key": "buildid"
                },
                "BUILD_TYPE": {
                    "value": "tinderbox"
                },
                "LOCALE": {
                    "key": "locale"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                }
            }
        },
        "by_branch": {
            "mozilla-aurora": {
                "locales": [
                    "fa",
                    "ru",
                    "zh-CN"
                ],
                "testruns": [
                    "l10n"
                ],
                "platforms": {
                    "linux64": [
                        "linux && ubuntu && 14.04 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.9 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            }
        }
    }
}
