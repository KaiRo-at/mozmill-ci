{
    "pulse": {
        "applabel": "qa-auto@mozilla.com|mozmill_l10n",
        "routing_key_regex": "build\\..+-l10n-dep\\.\\d+\\.finished",
        "platforms": [
            "linux",
            "win32"
        ],
        "branches": [
            "mozilla-aurora"
        ],
        "products": [
            "firefox"
        ],
        "locales": [
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "BUILD_ID": {
                    "key": "buildid"
                },
                "BUILD_TYPE": {
                    "value": "tinderbox"
                },
                "LOCALE": {
                    "key": "locale",
                    "default": "en-US"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                },
                "REPORT_URL": {
                    "value": "http://mozauto.iriscouch.com/mozmill-ci/"
                }
            }
        },
        "by_branch": {
            "mozilla-aurora": {
                "testruns": [
                    "l10n"
                ],
                "platforms": {
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 7 && 32bit"
                    ],
                    "linux": [
                        "linux && ubuntu && 12.10 && 32bit"
                    ]
                }
            }
        }
    }
}
