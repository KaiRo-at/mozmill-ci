{
    "pulse": {
        "applabel": "mozmill_release",
        "platforms": [
            "linux",
            "linux64",
            "macosx",
            "macosx64",
            "win32",
            "win64"
        ],
        "products": [
            "firefox"
        ],
        "tags": [
        ],
        "trees": [
            "release-mozilla-beta",
            "release-mozilla-release",
            "release-mozilla-esr38"
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "APPLICATION": {
                    "key": "product"
                },
                "BRANCH": {
                    "key": "tree"
                },
                "BUILD_NUMBER": {
                    "key": "build_number"
                },
                "BUILD_TYPE": {
                    "value": "candidate"
                },
                "BUILD_VERSION": {
                    "key": "version"
                },
                "LOCALE": {
                    "key": "locale"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                }
            }
        },
        "by_branch": {
            "release-mozilla-beta": {
                "locales": [
                    "ar",
                    "de",
                    "en-US",
                    "es-ES",
                    "fr",
                    "it",
                    "pl",
                    "pt-BR",
                    "ru"
                ],
                "testruns": [
                    "functional"
                ],
                "platforms": {
                    "linux": [
                        "linux && ubuntu && 14.04 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 14.04 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.6 && 64bit",
                        "mac && 10.9 && 64bit",
                        "mac && 10.10 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 32bit",
                        "windows && 8.1 && 64bit"
                    ],
                    "win64": [
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            },
            "release-mozilla-release": {
                "locales": [
                    "ar",
                    "de",
                    "en-US",
                    "es-ES",
                    "fr",
                    "it",
                    "pl",
                    "pt-BR",
                    "ru"
                ],
                "testruns": [
                    "functional"
                ],
                "platforms": {
                    "linux": [
                        "linux && ubuntu && 14.04 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 14.04 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.6 && 64bit",
                        "mac && 10.9 && 64bit",
                        "mac && 10.10 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 32bit",
                        "windows && 8.1 && 64bit"
                    ],
                    "win64": [
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            },
            "release-mozilla-esr38": {
                "locales": [
                    "en-US"
                ],
                "testruns": [
                    "functional"
                ],
                "platforms": {
                    "linux": [
                        "linux && ubuntu && 14.04 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 14.04 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.6 && 64bit",
                        "mac && 10.9 && 64bit",
                        "mac && 10.10 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 32bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            }
        }
    }
}
