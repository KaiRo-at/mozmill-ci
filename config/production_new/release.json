{
    "pulse": {
        "applabel": "qa-auto@mozilla.com|mozmill_release",
        "routing_key_regex": "build\\..+_build\\.\\d+\\.finished",
        "platforms": [
            "linux",
            "linux64",
            "win32"
        ],
        "branches": [
            "release-mozilla-beta",
            "release-mozilla-release",
            "release-mozilla-esr17",
            "release-mozilla-esr10"
        ],
        "products": [
            "firefox"
        ],
        "locales": [
            "en-US"
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "BUILD_NUMBER": {
                    "key": "build_number"
                },
                "BUILD_TYPE": {
                    "value": "candidate"
                },
                "LOCALE": {
                    "key": "locale",
                    "default": "en-US"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                },
                "REPORT_URL": {
                    "value": "http://mozauto.iriscouch.com/mozmill-ci/"
                },
                "VERSION": {
                    "key": "version"
                }
            },
            "endurance": {
                "ENTITIES": {
                    "value": 10
                },
                "ITERATIONS": {
                    "value": 10
                }
            },
            "update": {
                "BUILD_ID": {
                    "key": "previous_buildid"
                },
                "TARGET_BUILD_ID": {
                    "key": "buildid"
                }
            }
        },
        "by_branch": {
            "release-mozilla-beta": {
                "testruns": [
                    "functional",
                    "endurance",
                    "remote",
                    "addons"
                ],
                "platforms": {
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && vista && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8 && 32bit",
                        "windows && 8 && 64bit"
                    ],
                    "linux": [
                        "linux && ubuntu && 12.04 && 32bit",
                        "linux && ubuntu && 12.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 12.04 && 64bit",
                        "linux && ubuntu && 12.10 && 64bit"
                    ]
                }
            },
            "release-mozilla-release": {
                "testruns": [
                    "functional",
                    "endurance",
                    "remote",
                    "addons"
                ],
                "platforms": {
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && vista && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8 && 32bit",
                        "windows && 8 && 64bit"
                    ],
                    "linux": [
                        "linux && ubuntu && 12.04 && 32bit",
                        "linux && ubuntu && 12.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 12.04 && 64bit",
                        "linux && ubuntu && 12.10 && 64bit"
                    ]
                }
            },
            "release-mozilla-esr17": {
                "testruns": [
                    "functional",
                    "endurance",
                    "remote",
                    "addons"
                ],
                "platforms": {
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && vista && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8 && 32bit",
                        "windows && 8 && 64bit"
                    ],
                    "linux": [
                        "linux && ubuntu && 12.04 && 32bit",
                        "linux && ubuntu && 12.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 12.04 && 64bit",
                        "linux && ubuntu && 12.10 && 64bit"
                    ]
                }
            },
            "release-mozilla-esr10": {
                "testruns": [
                    "functional",
                    "endurance",
                    "remote",
                    "addons"
                ],
                "platforms": {
                    "win32": [
                        "windows && 2000 && 32bit",
                        "windows && xp && 32bit",
                        "windows && vista && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8 && 32bit",
                        "windows && 8 && 64bit"
                    ],
                    "linux": [
                        "linux && ubuntu && 12.04 && 32bit",
                        "linux && ubuntu && 12.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 12.04 && 64bit",
                        "linux && ubuntu && 12.10 && 64bit"
                    ]
                }
            }
        }
    }
}
