{
    "pulse": {
        "applabel": "mozmill_daily",
        "branches": [
            "mozilla-central",
            "mozilla-aurora"
        ],
        "platforms": [
            "linux",
            "linux64",
            "macosx",
            "macosx64",
            "win32",
            "win64"
        ],
        "products": [
            "firefox"
        ],
        "tags": [
            "nightly"
        ]
    },
    "jenkins": {
        "url": "http://localhost:8080",
        "username": "mozilla",
        "password": "test1234"
    },
    "testrun": {
        "jenkins_parameter_map": {
            "default": {
                "APPLICATION": {
                    "key": "product"
                },
                "BRANCH": {
                    "key": "branch"
                },
                "BUILD_ID": {
                    "key": "buildid"
                },
                "BUILD_TYPE": {
                    "value": "daily"
                },
                "LOCALE": {
                    "key": "locale"
                },
                "PLATFORM": {
                    "key": "platform",
                    "transform": "get_platform_identifier"
                },
                "REPORT_URL": {
                    "value": "http://mozauto.iriscouch.com/mozmill-daily/"
                }
            },
            "update": {
                "BUILD_ID": {
                    "key": "previous_buildid"
                },
                "TARGET_BUILD_ID": {
                    "key": "buildid"
                }
            }
        },
        "by_branch": {
            "project": {
               "locales": [
                    "en-US"
                ],
                "testruns": [
                    "update",
                    "functional",
                    "remote"
                ],
                "platforms": {
                    "linux": [
                        "linux && ubuntu && 14.04 && 32bit",
                        "linux && ubuntu && 14.10 && 32bit"
                    ],
                    "linux64": [
                        "linux && ubuntu && 14.04 && 64bit",
                        "linux && ubuntu && 14.10 && 64bit"
                    ],
                    "mac": [
                        "mac && 10.6 && 64bit",
                        "mac && 10.8 && 64bit",
                        "mac && 10.9 && 64bit",
                        "mac && 10.10 && 64bit"
                    ],
                    "win32": [
                        "windows && xp && 32bit",
                        "windows && 7 && 32bit",
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 32bit",
                        "windows && 8.1 && 64bit"
                    ],
                    "win64": [
                        "windows && 7 && 64bit",
                        "windows && 8.1 && 64bit"
                    ]
                }
            },
